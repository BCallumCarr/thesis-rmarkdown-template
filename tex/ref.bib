journal = {Journal of Educational Psychology},
keywords = {Job satisfaction,School climate,Social-emotional learning,Teacher stress,Teaching efficacy},
number = {4},
pages = {1189--1204},
pmid = {83480121},
title = {{School climate and social-emotional learning: Predicting teacher stress, job satisfaction, and teaching efficacy}},
volume = {104},
year = {2012}
}
