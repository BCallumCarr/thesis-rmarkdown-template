file = {:C$\backslash$:/Users/Brad/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Angrist, Pischke - 2008 - Mostly Harmless Econometrics An Empiricist's Companion.pdf:pdf},
isbn = {1111111111},
issn = {1556-5068},
pages = {290},
pmid = {20926156},
publisher = {Princeton University Press},
title = {{Mostly Harmless Econometrics: An Empiricist's Companion}},
year = {2008}
}
